{
  "name": "angular-quickstart",
  "version": "1.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "build:watch": "tsc -p src/ -w",
    "build:e2e": "tsc -p e2e/",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "prestart": "npm run build",
    "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "pree2e": "npm run build:e2e",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "pretest": "npm run build",
    "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build",
    "test:once": "karma start karma.conf.js --single-run",
    "cleanup": "rimraf app/**/*.ngfactory.* app/**/*.ngstyle.* ../VMSW.Conditiemeting.Webapplication/static/*.css ../VMSW.Conditiemeting.Webapplication/static/*.js ../VMSW.Conditiemeting.Webapplication/static/*.map app/**/*.ngsummary.* build dist decl",
    "build:prod": "npm run cleanup && cross-env NODE_ENV=production webpack --progress --config /src/webpack.config.js && ncp dist ../VMSW.Conditiemeting.Webapplication/static",
    "build:serve": "npm run cleanup && cross-env NODE_ENV=devserver webpack",
    "build": "npm run cleanup && cross-env NODE_ENV=dev webpack --progress && ncp ./src/dist ./static",
    "serve": "npm run build:serve && cross-env NODE_ENV=devserver webpack-dev-server --inline --hot --watch --progress --port 8081 --https",
    "lint": "tslint --config tslint.json 'app/**/*.ts' --exclude app/dto/*.ts --exclude app/polyfills.ts"

  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~4.0.0",
    "@angular/compiler": "~4.0.0",
    "@angular/core": "~4.0.0",
    "@angular/forms": "~4.0.0",
    "@angular/http": "~4.0.0",
    "@angular/platform-browser": "~4.0.0",
    "@angular/platform-browser-dynamic": "~4.0.0",
    "@angular/router": "~4.0.0",

    "angular-in-memory-web-api": "~0.3.0",
    "systemjs": "0.19.40",
    "core-js": "^2.4.1",
    "rxjs": "5.0.1",
    "zone.js": "^0.8.4",

    "@types/node": "^6.0.45",
    "@types/openlayers": "3.18.38",
    "angular-router-loader": "^0.5.0",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.1",

    "codelyzer": "^2.0.0",
    "concurrently": "^3.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.2.3",
    "css-loader": "^0.26.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "font-awesome": "4.7.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "ncp": "^2.0.0",
    "node-sass": "^4.5.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "tslint": "^4.5.1",
    "typescript": "~2.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^4.0.0",
    "webpack-node-externals": "^1.5.4"
  },
  "devDependencies": {
    "concurrently": "^3.2.0",
    "lite-server": "^2.2.2",
    "typescript": "~2.1.0",

    "canonical-path": "0.0.2",
    "tslint": "^3.15.1",
    "jasmine-core": "~2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~4.0.14",
    "rimraf": "^2.5.4",

    "@types/node": "^6.0.46",
    "@types/jasmine": "2.5.36"
  },
  "repository": {}
}
